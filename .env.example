# Setup token for initial passkey registration (random one generated if not set)
SETUP_TOKEN=
# UI server port (default: 3001)
PORT=3001
# Daemon socket path (default: /tmp/katulong-daemon.sock)
KATULONG_SOCK=/tmp/katulong-daemon.sock
# Log verbosity: debug, info, warn, error (default: info)
LOG_LEVEL=info
# Directory for persistent state files (default: ~/.katulong)
KATULONG_DATA_DIR=
# SSH server port (default: 2222)
SSH_PORT=2222
# SSH password (default: SETUP_TOKEN value)
SSH_PASSWORD=
